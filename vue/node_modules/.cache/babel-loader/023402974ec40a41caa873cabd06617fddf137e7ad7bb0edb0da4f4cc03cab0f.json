{"ast":null,"code":"import request from \"@/utils/request\";\nimport Cookies from 'js-cookie';\nexport default {\n  name: 'Video',\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      params: {\n        pageNum: 1,\n        pageSize: 10,\n        roleName: '',\n        telephone: ''\n      },\n      name: \"\",\n      form: {},\n      dialogFormVisible: false,\n      menuDialogVis: false,\n      multipleSelection: [],\n      menuData: [],\n      props: {\n        label: 'name'\n      },\n      expends: [],\n      checks: [],\n      roleId: 0,\n      roleFlag: '',\n      ids: []\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    /**\r\n     * 默认加载全部\r\n     */\n    load() {\n      request.get('/role/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === '200') {\n          this.tableData = res.data.list;\n          this.total = res.data.total;\n        }\n      });\n    },\n    /**\r\n     * 点击重置按钮恢复全部查询\r\n     */\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 10\n      };\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      // 点击分页按钮触发分页\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n    saveRoleMenu() {\n      this.request.post(\"/role/roleMenu/\" + this.roleId, this.$refs.tree.getCheckedKeys()).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"绑定成功\");\n          this.menuDialogVis = false;\n\n          // 操作管理员角色后需要重新登录\n          if (this.roleFlag === 'ROLE_ADMIN') {\n            this.$store.commit(\"logout\");\n          }\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    //风格函数，使文字居中\n    rowStyle() {\n      return \"text-align:center\";\n    }\n  }\n};","map":{"version":3,"mappings":"AAqEA;AACA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;QACAC;QACAC;QACAC;QACAC;MACA;MACAR;MACAS;MACAC;MACAC;MACAC;MACAC;MACAC;QACAC;MACA;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;EACA;EACAC;IACA;AACA;AACA;IACAC;MACAC;QACApB;MACA;QACA;UACA;UACA;QACA;MACA;IACA;IACA;AACA;AACA;IACAqB;MACA;QACApB;QACAC;MACA;MACA;IACA;IACAoB;MACA;MACA;MACA;IACA;IACAC;MACA;QACA;UACA;UACA;;UAEA;UACA;YACA;UACA;QAEA;UACA;QACA;MACA;IACA;IACA;IACAC;MACA;IACA;EACA;AACA","names":["name","data","tableData","total","params","pageNum","pageSize","roleName","telephone","form","dialogFormVisible","menuDialogVis","multipleSelection","menuData","props","label","expends","checks","roleId","roleFlag","ids","created","methods","load","request","reset","handleCurrentChange","saveRoleMenu","rowStyle"],"sourceRoot":"src/views/role","sources":["Role.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>用户管理</el-breadcrumb-item>\r\n      <el-breadcrumb-item>用户信息</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <el-main>\r\n      <div style=\"padding: 10px 0;\">\r\n        <el-input\r\n            placeholder=\"请输入名称\"\r\n            suffix-icon=\"el-icon-search\"\r\n            v-model=\"params.roleName\"\r\n            style=\"width: 200px; margin-top: 0px\">\r\n        </el-input>\r\n        <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"load\"><i class=\"el-icon-search\"></i> 搜索</el-button>\r\n        <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\"><i class=\"el-icon-refresh\"></i> 重置</el-button>\r\n        <el-button type=\"primary\" icon=\"el-icon-circle-plus-outline\" style=\"float: right\" @click=\"handleAdd()\">新增\r\n        </el-button>\r\n      </div>\r\n      <el-table :data=\"tableData\"\r\n                :cell-style=\"rowStyle\"\r\n                stripe row-key=\"userId\"\r\n                default-expand-all>\r\n        <el-table-column prop=\"roleId\" label=\"序号\" width=\"80\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"roleName\" label=\"角色名\" align=\"center\"></el-table-column>\r\n        <el-table-column label=\"操作\" align=\"center\">\r\n          <template v-slot=\"scope\">\r\n            <!--          scope.row 就是当前行数据-->\r\n            <el-button type=\"info\" @click=\"selectMenu(scope.row)\">分配菜单 <i class=\"el-icon-menu\"></i></el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <!--    分页-->\r\n      <div style=\"margin-top: 20px\">\r\n        <el-pagination\r\n            background\r\n            :current-page=\"params.pageNum\"\r\n            :page-size=\"params.pageSize\"\r\n            layout=\"prev, pager, next\"\r\n            @current-change=\"handleCurrentChange\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n    </el-main>\r\n\r\n    <el-dialog title=\"菜单分配\" :visible.sync=\"menuDialogVis\" width=\"30%\">\r\n      <el-tree\r\n          :props=\"props\"\r\n          :data=\"menuData\"\r\n          show-checkbox\r\n          node-key=\"id\"\r\n          ref=\"tree\"\r\n          :default-expanded-keys=\"expends\"\r\n          :default-checked-keys=\"checks\">\r\n         <span class=\"custom-tree-node\" slot-scope=\"{ node, data }\">\r\n            <span><i :class=\"data.icon\"></i> {{ data.name }}</span>\r\n         </span>\r\n      </el-tree>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"menuDialogVis = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"saveRoleMenu\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport Cookies from 'js-cookie'\r\n\r\nexport default {\r\n  name: 'Video',\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      params: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        roleName: '',\r\n        telephone: ''\r\n      },\r\n      name: \"\",\r\n      form: {},\r\n      dialogFormVisible: false,\r\n      menuDialogVis: false,\r\n      multipleSelection: [],\r\n      menuData: [],\r\n      props: {\r\n        label: 'name',\r\n      },\r\n      expends: [],\r\n      checks: [],\r\n      roleId: 0,\r\n      roleFlag: '',\r\n      ids: []\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    /**\r\n     * 默认加载全部\r\n     */\r\n    load() {\r\n      request.get('/role/page', {\r\n        params: this.params\r\n      }).then(res => {\r\n        if (res.code === '200') {\r\n          this.tableData = res.data.list\r\n          this.total = res.data.total\r\n        }\r\n      })\r\n    },\r\n    /**\r\n     * 点击重置按钮恢复全部查询\r\n     */\r\n    reset() {\r\n      this.params = {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n      }\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      // 点击分页按钮触发分页\r\n      this.params.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    saveRoleMenu() {\r\n      this.request.post(\"/role/roleMenu/\" + this.roleId, this.$refs.tree.getCheckedKeys()).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"绑定成功\")\r\n          this.menuDialogVis = false\r\n\r\n          // 操作管理员角色后需要重新登录\r\n          if (this.roleFlag === 'ROLE_ADMIN') {\r\n            this.$store.commit(\"logout\")\r\n          }\r\n\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    //风格函数，使文字居中\r\n    rowStyle() {\r\n      return \"text-align:center\";\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}