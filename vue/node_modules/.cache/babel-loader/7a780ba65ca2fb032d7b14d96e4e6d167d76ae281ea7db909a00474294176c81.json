{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request\";\nimport Cookies from 'js-cookie';\nexport default {\n  name: 'Video',\n  data() {\n    return {\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\n      tableData: [{\n        id: 1,\n        number: '893472472',\n        stationName: '易电石油大学电站',\n        stationAddress: '四川省成都市新都区新都大道8号',\n        chargeNumber: 5,\n        carsNumber: 5,\n        status: true\n      }, {\n        id: 1,\n        number: '893472472',\n        stationName: '易电石油大学电站',\n        stationAddress: '四川省成都市新都区新都大道8号',\n        chargeNumber: 5,\n        carsNumber: 5,\n        status: 0\n      }, {\n        id: 1,\n        number: '893472472',\n        stationName: '易电石油大学电站',\n        stationAddress: '四川省成都市新都区新都大道8号',\n        chargeNumber: 5,\n        carsNumber: 5,\n        status: true\n      }, {\n        id: 1,\n        number: '893472472',\n        stationName: '易电石油大学电站',\n        stationAddress: '四川省成都市新都区新都大道8号',\n        chargeNumber: 5,\n        carsNumber: 5,\n        status: 1\n      }],\n      total: 0,\n      params: {\n        pageNum: 1,\n        pageSize: 10,\n        name: '',\n        stationNo: ''\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    /**\r\n     * 默认加载全部\r\n     */\n    load() {\n      request.get('/station/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === '200') {\n          this.tableData = res.data.list;\n          this.total = res.data.total;\n        }\n      });\n    },\n    /**\r\n     * 点击重置按钮恢复全部查询\r\n     */\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 10,\n        bookName: '',\n        bookNo: '',\n        userName: ''\n      };\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      // 点击分页按钮触发分页\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n    /**\r\n     * 删除当前行\r\n     * @param id\r\n     */\n    del(id) {\n      request.delete(\"/station/delete/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('删除成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    /**\r\n     * 改变状态\r\n     * @param row\r\n     */\n    changeStatus(row) {\n      request.put('/station/update', row).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('操作成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    //添加函数，跳转到添加列表\n    handleAdd() {\n      this.$router.push();\n    },\n    //风格函数，使文字居中\n    rowStyle() {\n      return \"text-align:center\";\n    }\n  }\n};","map":{"version":3,"mappings":";AAyEA;AACA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;MACA,GACA;QACAN;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;MACA,GACA;QACAN;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;MACA,GACA;QACAN;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;MACA;MACAC;MACAC;QACAC;QACAC;QACAd;QACAe;MACA;IACA;EACA;EACAC;IACA;EACA;EACAC;IACA;AACA;AACA;IACAC;MACAC;QACAP;MACA;QACA;UACA;UACA;QACA;MACA;IACA;IACA;AACA;AACA;IACAQ;MACA;QACAP;QACAC;QACAO;QACAC;QACAC;MACA;MACA;IACA;IACAC;MACA;MACA;MACA;IACA;IAEA;AACA;AACA;AACA;IACAC;MACAN;QACA;UACA;UACA;QACA;UACA;QACA;MACA;IACA;IACA;AACA;AACA;AACA;IACAO;MACAP;QACA;UACA;UACA;QACA;UACA;QACA;MACA;IACA;IAEA;IACAQ;MACA;IACA;IAEA;IACAC;MACA;IACA;EACA;AACA","names":["name","data","admin","tableData","id","number","stationName","stationAddress","chargeNumber","carsNumber","status","total","params","pageNum","pageSize","stationNo","created","methods","load","request","reset","bookName","bookNo","userName","handleCurrentChange","del","changeStatus","handleAdd","rowStyle"],"sourceRoot":"src/views/station","sources":["station.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>电站管理</el-breadcrumb-item>\r\n      <el-breadcrumb-item>电站信息</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <el-main>\r\n      <div style=\"padding: 10px 0;\">\r\n        <el-input\r\n            placeholder=\"请输入内容\"\r\n            suffix-icon=\"el-icon-search\"\r\n            v-model=\"input\"\r\n            style=\"width: 200px; margin-top: 0px\">\r\n        </el-input>\r\n        <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"load\"><i class=\"el-icon-search\"></i> 搜索</el-button>\r\n        <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\"><i class=\"el-icon-refresh\"></i> 重置</el-button>\r\n        <el-button type=\"primary\" icon=\"el-icon-circle-plus-outline\" style=\"float: right\" @click=\"handleAdd()\">新增\r\n        </el-button>\r\n      </div>\r\n      <el-table :data=\"tableData\"\r\n                :cell-style=\"rowStyle\"\r\n                stripe row-key=\"id\"\r\n                default-expand-all>\r\n        <el-table-column prop=\"id\" label=\"序号\" width=\"80\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"number\" label=\"换电站编号\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"stationName\" label=\"换电站名称\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"stationAddress\" label=\"换电站地址\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"chargeNumber\" label=\"可用电池数量\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"chargeTotal\" label=\"总电池数量\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"status\" label=\"状态\" align=\"center\">\r\n          <template v-slot=\"scope\">\r\n            <el-switch\r\n                v-model= 'scope.row.status'\r\n                @change=\"changeStatus(scope.row)\"\r\n                active-color=\"#13ce66\"\r\n                inactive-color=\"#ff4949\"\r\n                active-value = '1'>\r\n            </el-switch>\r\n          </template>\r\n        </el-table-column>\r\n        <!--      <el-table-column prop=\"updatetime\" label=\"更新时间\"></el-table-column>-->\r\n        <el-table-column label=\"操作\" align=\"center\">\r\n          <template v-slot=\"scope\">\r\n            <!--          scope.row 就是当前行数据-->\r\n            <el-button type=\"primary\" @click=\"$router.push('/editStation?id=' + scope.row.id)\">编辑</el-button>\r\n            <el-popconfirm\r\n                style=\"margin-left: 5px\"\r\n                title=\"您确定删除这行数据吗？\"\r\n                @confirm=\"del(scope.row.id)\"\r\n            >\r\n              <el-button type=\"danger\" slot=\"reference\">删除</el-button>\r\n            </el-popconfirm>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <!--    分页-->\r\n      <div style=\"margin-top: 20px\">\r\n        <el-pagination\r\n            background\r\n            :current-page=\"params.pageNum\"\r\n            :page-size=\"params.pageSize\"\r\n            layout=\"prev, pager, next\"\r\n            @current-change=\"handleCurrentChange\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n    </el-main>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport Cookies from 'js-cookie'\r\n\r\nexport default {\r\n  name: 'Video',\r\n  data() {\r\n    return {\r\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\r\n      tableData: [{\r\n        id: 1,\r\n        number: '893472472',\r\n        stationName: '易电石油大学电站',\r\n        stationAddress: '四川省成都市新都区新都大道8号',\r\n        chargeNumber: 5,\r\n        carsNumber: 5,\r\n        status: true\r\n      },\r\n        {\r\n          id: 1,\r\n          number: '893472472',\r\n          stationName: '易电石油大学电站',\r\n          stationAddress: '四川省成都市新都区新都大道8号',\r\n          chargeNumber: 5,\r\n          carsNumber: 5,\r\n          status: 0\r\n        },\r\n        {\r\n          id: 1,\r\n          number: '893472472',\r\n          stationName: '易电石油大学电站',\r\n          stationAddress: '四川省成都市新都区新都大道8号',\r\n          chargeNumber: 5,\r\n          carsNumber: 5,\r\n          status: true\r\n        },\r\n        {\r\n          id: 1,\r\n          number: '893472472',\r\n          stationName: '易电石油大学电站',\r\n          stationAddress: '四川省成都市新都区新都大道8号',\r\n          chargeNumber: 5,\r\n          carsNumber: 5,\r\n          status: 1\r\n        },],\r\n      total: 0,\r\n      params: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        name: '',\r\n        stationNo: ''\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    /**\r\n     * 默认加载全部\r\n     */\r\n    load() {\r\n      request.get('/station/page', {\r\n        params: this.params\r\n      }).then(res => {\r\n        if (res.code === '200') {\r\n          this.tableData = res.data.list\r\n          this.total = res.data.total\r\n        }\r\n      })\r\n    },\r\n    /**\r\n     * 点击重置按钮恢复全部查询\r\n     */\r\n    reset() {\r\n      this.params = {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        bookName: '',\r\n        bookNo: '',\r\n        userName: ''\r\n      }\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      // 点击分页按钮触发分页\r\n      this.params.pageNum = pageNum\r\n      this.load()\r\n    },\r\n\r\n    /**\r\n     * 删除当前行\r\n     * @param id\r\n     */\r\n    del(id) {\r\n      request.delete(\"/station/delete/\" + id).then(res => {\r\n        if (res.code === '200') {\r\n          this.$notify.success('删除成功')\r\n          this.load()\r\n        } else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    /**\r\n     * 改变状态\r\n     * @param row\r\n     */\r\n    changeStatus(row){\r\n      request.put('/station/update', row).then(res => {\r\n        if (res.code === '200') {\r\n          this.$notify.success('操作成功')\r\n          this.load()\r\n        } else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n\r\n    //添加函数，跳转到添加列表\r\n    handleAdd(){\r\n      this.$router.push()\r\n    },\r\n\r\n    //风格函数，使文字居中\r\n    rowStyle() {\r\n      return \"text-align:center\";\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}