{"ast":null,"code":"import request from \"@/utils/request\";\nimport Cookies from 'js-cookie';\nexport default {\n  name: 'TroubleList',\n  data() {\n    return {\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\n      tableData: [{\n        troubleId: 1,\n        stationId: 734672,\n        troubleDescribe: '线路故障',\n        stationName: '易电石油大学站',\n        createTime: '2023-2-8',\n        resolveDate: '2023-2-9',\n        userId: 1,\n        note: '已处理'\n      }, {\n        troubleId: 2,\n        stationId: 734672,\n        troubleDescribe: '停电',\n        stationName: '易电石油大学站',\n        createTime: '2023-2-8',\n        note: '未处理'\n      }],\n      total: 0,\n      params: {\n        pageNum: 1,\n        pageSize: 10,\n        name: '',\n        bookNo: ''\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get('/trouble/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === '200') {\n          this.tableData = res.data.list;\n          this.total = res.data.total;\n        }\n      });\n    },\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 10,\n        bookName: '',\n        bookNo: '',\n        userName: ''\n      };\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      // 点击分页按钮触发分页\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n    del(troubleId) {\n      request.delete(\"/trouble/delete/\" + troubleId).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('删除成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    returnBooks(row) {\n      request.post(\"/trouble/saveRetur\", row).then(res => {\n        row.userId = this.admin.userId;\n        if (res.code === '200') {\n          this.$notify.success('还书成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAqEA;AACA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;MACA,GACA;QACAP;QACAC;QACAC;QACAC;QACAC;QACAG;MACA;MACAC;MACAC;QACAC;QACAC;QACAf;QACAgB;MACA;IACA;EACA;EACAC;IACA;EACA;EACAC;IACAC;MACAC;QACAP;MACA;QACA;UACA;UACA;QACA;MACA;IACA;IACAQ;MACA;QACAP;QACAC;QACAO;QACAN;QACAO;MACA;MACA;IACA;IACAC;MACA;MACA;MACA;IACA;IACAC;MACAL;QACA;UACA;UACA;QACA;UACA;QACA;MACA;IACA;IACAM;MACAN;QACAO;QACA;UACA;UACA;QACA;UACA;QACA;MACA;IACA;EACA;AACA","names":["name","data","admin","tableData","troubleId","stationId","troubleDescribe","stationName","createTime","resolveDate","userId","note","total","params","pageNum","pageSize","bookNo","created","methods","load","request","reset","bookName","userName","handleCurrentChange","del","returnBooks","row"],"sourceRoot":"src/views/trouble","sources":["trouble.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div>\r\n      <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n        <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n        <el-breadcrumb-item>故障管理</el-breadcrumb-item>\r\n      </el-breadcrumb>\r\n    </div>\r\n    <!--    搜索表单-->\r\n    <div style=\"margin-bottom: 20px; margin-top: 30px\">\r\n      <el-input style=\"width: 240px\" placeholder=\"请输入图书名称\" v-model=\"params.bookName\"></el-input>\r\n      <el-input style=\"width: 240px; margin-left: 5px\" placeholder=\"请输入图书标准码\" v-model=\"params.bookNo\"></el-input>\r\n      <el-input style=\"width: 240px; margin-left: 5px\" placeholder=\"请输入用户名称\" v-model=\"params.userName\"></el-input>\r\n      <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"load\"><i class=\"el-icon-search\"></i> 搜索</el-button>\r\n      <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\"><i class=\"el-icon-refresh\"></i> 重置</el-button>\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\" stripe row-key=\"troubleId\"  default-expand-all>\r\n      <el-table-column prop=\"troubleId\" label=\"故障编号\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"stationId\" label=\"换电站编号\"></el-table-column>\r\n      <el-table-column prop=\"troubleDescribe\" label=\"故障描述\"></el-table-column>\r\n      <el-table-column prop=\"stationName\" label=\"换电站地址\"></el-table-column>\r\n      <el-table-column prop=\"createTime\" label=\"上报日期\"></el-table-column>\r\n      <el-table-column prop=\"resolveDate\" label=\"处理日期\"></el-table-column>\r\n      <el-table-column prop=\"userName\" label=\"处理人姓名\"></el-table-column>\r\n      <el-table-column prop=\"telephone\" label=\"处理人手机号\"></el-table-column>\r\n      <el-table-column prop=\"note\" label=\"故障提示\">\r\n        <template v-slot=\"scope\">\r\n          <el-tag type=\"success\" v-if=\"scope.row.note === '已处理'\">{{ scope.row.note }}</el-tag>\r\n          <el-tag type=\"danger\" v-if=\"scope.row.note === '未处理'\">{{ scope.row.note }}</el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"管理\">\r\n        <template v-slot=\"scope\">\r\n          <el-button type=\"primary\" @click=\"returnBooks(scope.row)\" v-if=\"scope.row.note === '未处理'\">排除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n      <!--      <el-table-column prop=\"updatetime\" label=\"更新时间\"></el-table-column>-->\r\n      <el-table-column label=\"操作\">\r\n        <template v-slot=\"scope\">\r\n          <!--          scope.row 就是当前行数据-->\r\n          <!--          <el-button type=\"primary\" @click=\"$router.push('/editBorrow?troubleId=' + scope.row.troubleId)\">编辑</el-button>-->\r\n          <el-popconfirm\r\n              style=\"margin-left: 5px\"\r\n              title=\"您确定删除这行数据吗？\"\r\n              @confirm=\"del(scope.row.troubleId)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\">删除</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!--    分页-->\r\n    <div style=\"margin-top: 20px\">\r\n      <el-pagination\r\n          background\r\n          :current-page=\"params.pageNum\"\r\n          :page-size=\"params.pageSize\"\r\n          layout=\"prev, pager, next\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport Cookies from 'js-cookie'\r\n\r\nexport default {\r\n  name: 'TroubleList',\r\n  data() {\r\n    return {\r\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\r\n      tableData: [{\r\n        troubleId: 1,\r\n        stationId: 734672,\r\n        troubleDescribe: '线路故障',\r\n        stationName: '易电石油大学站',\r\n        createTime: '2023-2-8',\r\n        resolveDate: '2023-2-9',\r\n        userId: 1,\r\n        note: '已处理'\r\n      },\r\n        {\r\n          troubleId: 2,\r\n          stationId: 734672,\r\n          troubleDescribe: '停电',\r\n          stationName: '易电石油大学站',\r\n          createTime: '2023-2-8',\r\n          note: '未处理'\r\n        }],\r\n      total: 0,\r\n      params: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        name: '',\r\n        bookNo: ''\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load() {\r\n      request.get('/trouble/page', {\r\n        params: this.params\r\n      }).then(res => {\r\n        if (res.code === '200') {\r\n          this.tableData = res.data.list\r\n          this.total = res.data.total\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.params = {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        bookName: '',\r\n        bookNo: '',\r\n        userName: ''\r\n      }\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      // 点击分页按钮触发分页\r\n      this.params.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    del(troubleId) {\r\n      request.delete(\"/trouble/delete/\" + troubleId).then(res => {\r\n        if (res.code === '200') {\r\n          this.$notify.success('删除成功')\r\n          this.load()\r\n        } else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    returnBooks(row) {\r\n      request.post(\"/trouble/saveRetur\", row).then(res => {\r\n        row.userId = this.admin.userId;\r\n        if (res.code === '200') {\r\n          this.$notify.success('还书成功')\r\n          this.load()\r\n        } else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}