{"ast":null,"code":"import request from \"@/utils/request\";\nimport Cookies from 'js-cookie';\nexport default {\n  name: 'BorrowList',\n  data() {\n    return {\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\n      tableData: [],\n      total: 0,\n      params: {\n        pageNum: 1,\n        pageSize: 10,\n        name: '',\n        bookNo: ''\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get('/borrow/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === '200') {\n          this.tableData = res.data.list;\n          this.total = res.data.total;\n        }\n      });\n    },\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 10,\n        bookName: '',\n        bookNo: '',\n        userName: ''\n      };\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      // 点击分页按钮触发分页\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n    del(id) {\n      request.delete(\"/borrow/delete/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('删除成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    handleAdd() {},\n    returnBooks(row) {\n      request.post(\"/borrow/saveRetur\", row).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('还书成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA8DA;AACA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;QACAC;QACAC;QACAP;QACAQ;MACA;IACA;EACA;EACAC;IACA;EACA;EACAC;IACAC;MACAC;QACAP;MACA;QACA;UACA;UACA;QACA;MACA;IACA;IACAQ;MACA;QACAP;QACAC;QACAO;QACAN;QACAO;MACA;MACA;IACA;IACAC;MACA;MACA;MACA;IACA;IACAC;MACAL;QACA;UACA;UACA;QACA;UACA;QACA;MACA;IACA;IACAM,aAEA;IACAC;MACAP;QACA;UACA;UACA;QACA;UACA;QACA;MACA;IACA;EACA;AACA","names":["name","data","admin","tableData","total","params","pageNum","pageSize","bookNo","created","methods","load","request","reset","bookName","userName","handleCurrentChange","del","handleAdd","returnBooks"],"sourceRoot":"src/views/video","sources":["video.vue"],"sourcesContent":["<template>\r\n  <el-main>\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>视频管理</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <div style=\"padding: 10px 0;\">\r\n      <el-input\r\n          placeholder=\"请输入内容\"\r\n          suffix-icon=\"el-icon-search\"\r\n          v-model=\"input\"\r\n          style=\"width: 200px; margin-top: 0px\">\r\n      </el-input>\r\n      <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"load\"><i class=\"el-icon-search\"></i> 搜索</el-button>\r\n      <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\"><i class=\"el-icon-refresh\"></i> 重置</el-button>\r\n      <el-button type=\"primary\" icon=\"el-icon-circle-plus-outline\" style=\"float: right\" @click=\"handleAdd()\">新增\r\n      </el-button>\r\n    </div>\r\n    <el-table :data=\"tableData\" stripe row-key=\"id\"  default-expand-all>\r\n      <el-table-column prop=\"id\" label=\"编号\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"videoAddress\" label=\"视频流地址\"></el-table-column>\r\n      <el-table-column prop=\"origin\" label=\"来源\"></el-table-column>\r\n      <el-table-column prop=\"agreement\" label=\"流传输协议\"></el-table-column>\r\n      <el-table-column prop=\"bandWidth\" label=\"入口带宽\"></el-table-column>\r\n      <el-table-column prop=\"carsNumber\" label=\"实时排队车辆结果\"></el-table-column>\r\n      <el-table-column prop=\"status\" label=\"状态\">\r\n        <template v-slot=\"scope\">\r\n          <el-tag type=\"success\" v-if=\"scope.row.status === '上线'\">{{ scope.row.note }}</el-tag>\r\n          <el-tag type=\"danger\" v-if=\"scope.row.status === '下线'\">{{ scope.row.note }}</el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <!--      <el-table-column prop=\"updatetime\" label=\"更新时间\"></el-table-column>-->\r\n      <el-table-column label=\"操作\">\r\n        <template v-slot=\"scope\">\r\n          <!--          scope.row 就是当前行数据-->\r\n          <el-button type=\"primary\" @click=\"$router.push('/editBorrow?id=' + scope.row.id)\">编辑</el-button>\r\n          <el-popconfirm\r\n              style=\"margin-left: 5px\"\r\n              title=\"您确定删除这行数据吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\">删除</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!--    分页-->\r\n    <div style=\"margin-top: 20px\">\r\n      <el-pagination\r\n          background\r\n          :current-page=\"params.pageNum\"\r\n          :page-size=\"params.pageSize\"\r\n          layout=\"prev, pager, next\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n  </el-main>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport Cookies from 'js-cookie'\r\n\r\nexport default {\r\n  name: 'BorrowList',\r\n  data() {\r\n    return {\r\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\r\n      tableData: [],\r\n      total: 0,\r\n      params: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        name: '',\r\n        bookNo: ''\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load() {\r\n      request.get('/borrow/page', {\r\n        params: this.params\r\n      }).then(res => {\r\n        if (res.code === '200') {\r\n          this.tableData = res.data.list\r\n          this.total = res.data.total\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.params = {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        bookName: '',\r\n        bookNo: '',\r\n        userName: ''\r\n      }\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      // 点击分页按钮触发分页\r\n      this.params.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    del(id) {\r\n      request.delete(\"/borrow/delete/\" + id).then(res => {\r\n        if (res.code === '200') {\r\n          this.$notify.success('删除成功')\r\n          this.load()\r\n        } else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    handleAdd(){\r\n\r\n    },\r\n    returnBooks(row) {\r\n      request.post(\"/borrow/saveRetur\", row).then(res => {\r\n        if (res.code === '200') {\r\n          this.$notify.success('还书成功')\r\n          this.load()\r\n        } else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}