{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request\";\nimport Cookies from 'js-cookie';\nexport default {\n  name: 'Station',\n  data() {\n    return {\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\n      tableData: [],\n      total: 0,\n      params: {\n        pageNum: 1,\n        pageSize: 10,\n        stationName: '',\n        stationNumber: ''\n      },\n      dialogFormVisible: false,\n      options: [],\n      carIds: [],\n      form: {},\n      rules: {\n        stationName: [{\n          required: true,\n          message: '请输入换电站名称',\n          trigger: 'blur'\n        }, {\n          min: 3,\n          max: 10,\n          message: '长度在3-10个字符',\n          trigger: 'blur'\n        }],\n        stationAddress: [{\n          required: true,\n          message: '请输入换电站地址',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    /**\r\n     * 默认加载全部\r\n     */\n    load() {\n      if (this.admin.roleId === 2) {\n        this.params.stationNumber = this.admin.stationNumber;\n      }\n      request.get('/station/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === '200') {\n          this.tableData = res.data.list;\n          this.total = res.data.total;\n        }\n      });\n    },\n    /**\r\n     * 点击重置按钮恢复全部查询\r\n     */\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 10,\n        bookName: '',\n        bookNo: '',\n        userName: ''\n      };\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      // 点击分页按钮触发分页\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n    /**\r\n     * 删除当前行\r\n     * @param stationId\r\n     */\n    del(stationId) {\n      request.delete(\"/station/delete/\" + stationId).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('删除成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    /**\r\n     * 改变状态\r\n     * @param row\r\n     */\n    changeStatus(row) {\n      request.put('/station/updateStatus', row).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('操作成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    save() {\n      this.form.carIdList = [];\n      for (let i = 0; i < this.carIds.length; i++) {\n        this.form.carIdList.push(this.carIds[i][1]);\n      }\n      this.$refs['ruleForm'].validate(valid => {\n        if (valid) {\n          request.post('/station/add', this.form).then(res => {\n            if (res.code === '200') {\n              this.$notify.success('新增成功');\n              this.$refs['ruleForm'].resetFields();\n            } else {\n              this.$notify.error(res.msg);\n            }\n          });\n        }\n      });\n      this.dialogFormVisible = false;\n      this.$emit(this.load());\n    },\n    //添加函数，跳转到添加列表\n    handleAdd() {\n      request.get('station/carTreeList').then(res => {\n        if (res.code === '200') {\n          this.options = res.data;\n        }\n      });\n      this.dialogFormVisible = true;\n    },\n    //风格函数，使文字居中\n    rowStyle() {\n      return \"text-align:center\";\n    }\n  }\n};","map":{"version":3,"mappings":";AA2GA;AACA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;QACAC;QACAC;QACAC;QACAC;MACA;MACAC;MACAC;MACAC;MACAC;MACAC;QACAN,cACA;UAAAO;UAAAC;UAAAC;QAAA,GACA;UAAAC;UAAAC;UAAAH;UAAAC;QAAA,EACA;QACAG,iBACA;UAAAL;UAAAC;UAAAC;QAAA;MAEA;IACA;EACA;EACAI;IACA;EACA;EACAC;IACA;AACA;AACA;IACAC;MACA;QACA;MACA;MACAC;QACAnB;MACA;QACA;UACA;UACA;QACA;MACA;IACA;IACA;AACA;AACA;IACAoB;MACA;QACAnB;QACAC;QACAmB;QACAC;QACAC;MACA;MACA;IACA;IACAC;MACA;MACA;MACA;IACA;IAEA;AACA;AACA;AACA;IACAC;MACAN;QACA;UACA;UACA;QACA;UACA;QACA;MACA;IACA;IACA;AACA;AACA;AACA;IACAO;MACAP;QACA;UACA;UACA;QACA;UACA;QACA;MACA;IACA;IAEAQ;MACA;MACA;QACA;MACA;MACA;QACA;UACAR;YACA;cACA;cACA;YACA;cACA;YACA;UACA;QACA;MACA;MACA;MACA;IACA;IAEA;IACAS;MACAT;QACA;UACA;QACA;MACA;MACA;IACA;IAEA;IACAU;MACA;IACA;EACA;AACA","names":["name","data","admin","tableData","total","params","pageNum","pageSize","stationName","stationNumber","dialogFormVisible","options","carIds","form","rules","required","message","trigger","min","max","stationAddress","created","methods","load","request","reset","bookName","bookNo","userName","handleCurrentChange","del","changeStatus","save","handleAdd","rowStyle"],"sourceRoot":"src/views/station","sources":["station.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>电站管理</el-breadcrumb-item>\r\n      <el-breadcrumb-item>电站信息</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <el-main>\r\n      <div style=\"padding: 10px 0;\" v-show=\"admin.roleId === 1\">\r\n        <el-input placeholder=\"请输入换电站名称\" suffix-icon=\"el-icon-search\" v-model=\"params.stationName\" style=\"width: 200px; margin-top: 0px\"></el-input>\r\n        <el-input placeholder=\"请输入换电站编号\" suffix-icon=\"el-icon-search\" v-model=\"params.stationNumber\" style=\"width: 200px; margin-top: 0px\"></el-input>\r\n        <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"load\"><i class=\"el-icon-search\"></i> 搜索</el-button>\r\n        <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\"><i class=\"el-icon-refresh\"></i> 重置</el-button>\r\n        <el-button type=\"primary\" icon=\"el-icon-circle-plus-outline\" style=\"float: right\" @click=\"handleAdd\">新增</el-button>\r\n      </div>\r\n      <el-table :data=\"tableData\"\r\n                :cell-style=\"rowStyle\"\r\n                stripe row-key=\"stationId\"\r\n                default-expand-all>\r\n        <el-table-column prop=\"stationId\" label=\"序号\" width=\"80\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"stationNumber\" label=\"换电站编号\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"stationName\" label=\"换电站名称\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"stationAddress\" label=\"换电站地址\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"stationAltitude\" label=\"换电站经纬度\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"stationServetime\" label=\"换电站服务时长\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"stationFee\" label=\"换电站平均消费\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"chargeNumber\" label=\"可用电池数量\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"chargeTotal\" label=\"总电池数量\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"videoUrl\" label=\"视频流地址\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"status\" label=\"状态\" align=\"center\">\r\n          <template v-slot=\"scope\">\r\n            <el-switch\r\n                v-model= 'scope.row.status'\r\n                @change=\"changeStatus(scope.row)\"\r\n                active-color=\"#13ce66\"\r\n                inactive-color=\"#ff4949\"\r\n                :active-value = 1\r\n                :inactive-value = 0>\r\n            </el-switch>\r\n          </template>\r\n        </el-table-column>\r\n        <!--      <el-table-column prop=\"updatetime\" label=\"更新时间\"></el-table-column>-->\r\n        <el-table-column label=\"操作\" align=\"center\" width=\"200px\">\r\n          <template v-slot=\"scope\">\r\n            <!--          scope.row 就是当前行数据-->\r\n            <el-button type=\"primary\" @click=\"$router.push('/editStation?stationNumber=' + scope.row.stationNumber + '&stationId=' + scope.row.stationId)\">编辑</el-button>\r\n            <el-popconfirm\r\n                style=\"margin-left: 5px\"\r\n                title=\"您确定删除这行数据吗？\"\r\n                @confirm=\"del(scope.row.stationId)\"\r\n            >\r\n              <el-button type=\"danger\" slot=\"reference\" v-show=\"admin.roleId === 1\">删除</el-button>\r\n            </el-popconfirm>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <!--    分页-->\r\n      <div style=\"margin-top: 20px\">\r\n        <el-pagination\r\n            background\r\n            :current-page=\"params.pageNum\"\r\n            :page-size=\"params.pageSize\"\r\n            layout=\"prev, pager, next\"\r\n            @current-change=\"handleCurrentChange\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n\r\n        <el-dialog title=\"新增电站\" :visible.sync=\"dialogFormVisible\" width=\"700px\" center>\r\n          <el-form :inline=\"true\" :model=\"form\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"100px\">\r\n            <el-form-item label=\"换电站名称\" prop=\"stationName\">\r\n              <el-input v-model=\"form.stationName\" placeholder=\"请输入换电站名称\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"换电站地址\" prop=\"stationAddress\">\r\n              <el-input v-model=\"form.stationAddress\" placeholder=\"请输入换电站地址\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"换电站经纬度\" prop=\"stationAltitude\">\r\n              <el-input v-model=\"form.stationAltitude\" placeholder=\"请输入换电站经纬度\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"换电站服务时长\" prop=\"stationServetime\">\r\n              <el-input v-model=\"form.stationServetime\" placeholder=\"请输入换电站服务时长\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"换电站平均消费\" prop=\"stationFee\">\r\n              <el-input v-model=\"form.stationFee\" placeholder=\"请输入换电站平均消费\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"总电池数量\" prop=\"chargeTotal\">\r\n              <el-input v-model=\"form.chargeTotal\" placeholder=\"请输入总电池数量\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"支持的汽车\" prop=\"carIdList\">\r\n              <el-cascader\r\n                  :options=\"options\"\r\n                  :props=\"{ multiple: true }\"\r\n                  v-model=\"carIds\"\r\n                  clearable></el-cascader>\r\n            </el-form-item>\r\n          </el-form>\r\n          <div slot=\"footer\" class=\"dialog-footer\">\r\n            <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n            <el-button type=\"primary\" @click=\"save()\">确 定</el-button>\r\n          </div>\r\n        </el-dialog>\r\n    </el-main>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport Cookies from 'js-cookie'\r\n\r\nexport default {\r\n  name: 'Station',\r\n  data() {\r\n    return {\r\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\r\n      tableData: [],\r\n      total: 0,\r\n      params: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        stationName: '',\r\n        stationNumber: '',\r\n      },\r\n      dialogFormVisible: false,\r\n      options: [],\r\n      carIds:[],\r\n      form: {},\r\n      rules: {\r\n        stationName: [\r\n          { required: true, message: '请输入换电站名称', trigger: 'blur'},\r\n          { min: 3, max: 10, message: '长度在3-10个字符', trigger: 'blur'}\r\n        ],\r\n        stationAddress: [\r\n          { required:true, message: '请输入换电站地址', trigger: 'blur' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    /**\r\n     * 默认加载全部\r\n     */\r\n    load() {\r\n      if (this.admin.roleId === 2){\r\n        this.params.stationNumber = this.admin.stationNumber\r\n      }\r\n      request.get('/station/page', {\r\n        params: this.params\r\n      }).then(res => {\r\n        if (res.code === '200') {\r\n          this.tableData = res.data.list\r\n          this.total = res.data.total\r\n        }\r\n      })\r\n    },\r\n    /**\r\n     * 点击重置按钮恢复全部查询\r\n     */\r\n    reset() {\r\n      this.params = {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        bookName: '',\r\n        bookNo: '',\r\n        userName: ''\r\n      }\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      // 点击分页按钮触发分页\r\n      this.params.pageNum = pageNum\r\n      this.load()\r\n    },\r\n\r\n    /**\r\n     * 删除当前行\r\n     * @param stationId\r\n     */\r\n    del(stationId) {\r\n      request.delete(\"/station/delete/\" + stationId).then(res => {\r\n        if (res.code === '200') {\r\n          this.$notify.success('删除成功')\r\n          this.load()\r\n        } else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    /**\r\n     * 改变状态\r\n     * @param row\r\n     */\r\n    changeStatus(row){\r\n      request.put('/station/updateStatus', row).then(res => {\r\n        if (res.code === '200') {\r\n          this.$notify.success('操作成功')\r\n          this.load()\r\n        } else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n\r\n    save() {\r\n      this.form.carIdList = []\r\n      for (let i = 0; i < this.carIds.length; i++){\r\n        this.form.carIdList.push(this.carIds[i][1])\r\n      }\r\n      this.$refs['ruleForm'].validate((valid) => {\r\n        if (valid) {\r\n          request.post('/station/add', this.form).then(res => {\r\n            if (res.code === '200') {\r\n              this.$notify.success('新增成功')\r\n              this.$refs['ruleForm'].resetFields()\r\n            } else {\r\n              this.$notify.error(res.msg)\r\n            }\r\n          })\r\n        }\r\n      })\r\n      this.dialogFormVisible = false\r\n      this.$emit(this.load())\r\n    },\r\n\r\n    //添加函数，跳转到添加列表\r\n    handleAdd(){\r\n      request.get('station/carTreeList').then(res =>{\r\n        if (res.code === '200') {\r\n          this.options = res.data\r\n        }\r\n      })\r\n      this.dialogFormVisible = true\r\n    },\r\n\r\n    //风格函数，使文字居中\r\n    rowStyle() {\r\n      return \"text-align:center\";\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}