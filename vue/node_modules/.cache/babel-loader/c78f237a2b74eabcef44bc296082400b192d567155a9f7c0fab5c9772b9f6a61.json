{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request\";\nexport default {\n  name: 'EditStation',\n  data() {\n    return {\n      options: [],\n      defaultCars: [],\n      form: {}\n    };\n  },\n  created() {\n    this.getCarList();\n    const stationNumber = this.$route.query.stationNumber;\n    const stationId = this.$route.query.stationId;\n    request.get(\"/station/car/\" + stationId).then(res => {\n      let carsId = res.data;\n      for (let i = 0; i <= carsId.length; i++) {\n        let list = [0];\n        list.push(carsId[i]);\n        this.defaultCars.push(list);\n      }\n    });\n    request.get(\"/station/\" + stationNumber).then(res => {\n      this.form = res.data;\n    });\n  },\n  methods: {\n    save() {\n      request.put('/station/update', this.form).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('更新成功');\n          this.$router.push(\"/info\");\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    getCarList() {\n      request.get('station/carTreeList').then(res => {\n        if (res.code === '200') {\n          this.options = res.data;\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AAgDA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACA;IACA;IACAC;MACA;MACA;QACA;QACAC;QACA;MACA;IACA;IACAD;MACA;IACA;EACA;EACAE;IACAC;MACAH;QACA;UACA;UACA;QACA;UACA;QACA;MACA;IACA;IACAI;MACAJ;QACA;UACA;QACA;MACA;IACA;EACA;AACA","names":["name","data","options","defaultCars","form","created","request","list","methods","save","getCarList"],"sourceRoot":"src/views/station","sources":["stationEdit.vue"],"sourcesContent":["<template>\r\n  <div style=\"width: 80%\">\r\n    <div>\r\n      <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n        <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n        <el-breadcrumb-item :to=\"{ path: '/info' }\">电站管理</el-breadcrumb-item>\r\n        <el-breadcrumb-item>电站修改</el-breadcrumb-item>\r\n      </el-breadcrumb>\r\n    </div>\r\n    <div style=\"margin-top: 30px\">\r\n      <el-form :inline=\"true\" :model=\"form\" label-width=\"100px\">\r\n        <el-form-item label=\"换电站编号\">\r\n          <el-input v-model=\"form.stationNumber\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"换电站名称\">\r\n          <el-input v-model=\"form.stationName\" placeholder=\"请输入换电站名称\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"换电站地址\">\r\n          <el-input v-model=\"form.stationAddress\" placeholder=\"请输入换电站地址\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"可用电池数量\">\r\n          <el-input v-model=\"form.chargeNumber\" placeholder=\"请输入可用电池数量\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"总电池数量\">\r\n          <el-input v-model=\"form.chargeTotal\" placeholder=\"请输入总电池数量\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"视频流地址\">\r\n          <el-input v-model=\"form.videoUrl\" placeholder=\"请输入视频流地址\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"支持的汽车\">\r\n          <div class=\"block\">\r\n            <el-cascader\r\n                :options=\"options\"\r\n                :props=\"{ multiple: true}\"\r\n                v-model=\"this.defaultCars\"\r\n                clearable></el-cascader>\r\n          </div>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n\r\n    <div style=\"text-align: center; margin-top: 30px\">\r\n      <el-button type=\"primary\" @click=\"save\" size=\"medium\">提交</el-button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: 'EditStation',\r\n  data() {\r\n    return {\r\n      options:[],\r\n      defaultCars:[],\r\n      form: {}\r\n    }\r\n  },\r\n  created() {\r\n    this.getCarList()\r\n    const stationNumber = this.$route.query.stationNumber\r\n    const stationId = this.$route.query.stationId\r\n    request.get(\"/station/car/\" + stationId).then(res => {\r\n      let carsId = res.data\r\n      for (let i=0; i<=carsId.length; i++){\r\n        let list = [0]\r\n        list.push(carsId[i])\r\n        this.defaultCars.push(list)\r\n      }\r\n    })\r\n    request.get(\"/station/\" + stationNumber).then(res => {\r\n      this.form = res.data\r\n    })\r\n  },\r\n  methods: {\r\n    save() {\r\n      request.put('/station/update', this.form).then(res => {\r\n        if (res.code === '200') {\r\n          this.$notify.success('更新成功')\r\n          this.$router.push(\"/info\")\r\n        } else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    getCarList(){\r\n      request.get('station/carTreeList').then(res =>{\r\n        if (res.code === '200') {\r\n          this.options = res.data\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}