{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request\";\nimport Cookies from 'js-cookie';\nexport default {\n  name: 'Video',\n  data() {\n    return {\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\n      tableData: [],\n      total: 0,\n      params: {\n        pageNum: 1,\n        pageSize: 10,\n        videoUrl: '',\n        stationNumber: ''\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      this.params.stationNumber = this.admin.stationNumber;\n      request.get('/video/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === '200') {\n          this.tableData = res.data.list;\n          this.total = res.data.total;\n        }\n      });\n    },\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 10,\n        videoUrl: '',\n        stationNumber: this.admin.stationNumber\n      };\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      // 点击分页按钮触发分页\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n    del(videoId) {\n      request.delete(\"/video/delete/\" + videoId).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('删除成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    online(videoId) {\n      request.put(\"/video/online/\" + videoId).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('上线成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    handleAdd() {\n      this.$router.push(\"/videoAdd\");\n    },\n    rowStyle() {\n      return \"text-align:center\";\n    }\n  }\n};","map":{"version":3,"mappings":";AAwEA;AACA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;QACAC;QACAC;QACAC;QACAC;MACA;IACA;EACA;EACAC;IACA;EACA;EACAC;IACAC;MACA;MACAC;QACAR;MACA;QACA;UACA;UACA;QACA;MACA;IACA;IACAS;MACA;QACAR;QACAC;QACAC;QACAC;MACA;MACA;IACA;IACAM;MACA;MACA;MACA;IACA;IACAC;MACAH;QACA;UACA;UACA;QACA;UACA;QACA;MACA;IACA;IACAI;MACAJ;QACA;UACA;UACA;QACA;UACA;QACA;MACA;IACA;IACAK;MACA;IACA;IACAC;MACA;IACA;EACA;AACA","names":["name","data","admin","tableData","total","params","pageNum","pageSize","videoUrl","stationNumber","created","methods","load","request","reset","handleCurrentChange","del","online","handleAdd","rowStyle"],"sourceRoot":"src/views/video","sources":["video.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>视频管理</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <el-main>\r\n      <div style=\"padding: 10px 0;\">\r\n        <el-input\r\n            placeholder=\"请输入视频流地址\"\r\n            suffix-icon=\"el-icon-search\"\r\n            v-model=\"params.videoUrl\"\r\n            style=\"width: 200px; margin-top: 0px\">\r\n        </el-input>\r\n        <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"load\"><i class=\"el-icon-search\"></i> 搜索</el-button>\r\n        <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\"><i class=\"el-icon-refresh\"></i> 重置</el-button>\r\n        <el-button type=\"primary\" icon=\"el-icon-circle-plus-outline\" style=\"float: right\" @click=\"handleAdd()\">新增\r\n        </el-button>\r\n      </div>\r\n      <el-table :data=\"tableData\"\r\n                :cell-style=\"rowStyle\"\r\n                stripe row-key=\"videoId\"\r\n                default-expand-all>\r\n        <el-table-column prop=\"videoId\" label=\"编号\" width=\"80\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"videoUrl\" label=\"视频流地址\" align=\"center\" width=\"300px\"></el-table-column>\r\n        <el-table-column prop=\"origin\" label=\"来源\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"agreement\" label=\"流传输协议\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"bandWidth\" label=\"入口带宽\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"carNumber\" label=\"实时排队车辆结果\" align=\"center\">\r\n          <template v-slot=\"scope\">\r\n            <span v-if=\"scope.row.status === 1\">{{scope.row.carNumber}}</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"status\" label=\"状态\" align=\"center\">\r\n          <template v-slot=\"scope\">\r\n            <el-tag type=\"success\" v-if=\"scope.row.status === 1\">上线</el-tag>\r\n            <el-tag type=\"danger\" v-if=\"scope.row.status === 0\">下线</el-tag>\r\n          </template>\r\n        </el-table-column>\r\n        <!--      <el-table-column prop=\"updatetime\" label=\"更新时间\"></el-table-column>-->\r\n        <el-table-column label=\"操作\" align=\"center\" width=\"300px\">\r\n          <template v-slot=\"scope\">\r\n            <!--          scope.row 就是当前行数据-->\r\n            <el-button type=\"primary\" @click=\"$router.push('/editVideo?videoId=' + scope.row.videoId)\">编辑</el-button>\r\n            <el-button type=\"success\" @click=\"online(scope.row.videoId)\" v-if=\"scope.row.status === 0\">部署</el-button>\r\n            <el-popconfirm\r\n                style=\"margin-left: 5px\"\r\n                title=\"您确定删除这行数据吗？\"\r\n                @confirm=\"del(scope.row.videoId)\"\r\n            >\r\n              <el-button type=\"danger\" slot=\"reference\">删除</el-button>\r\n            </el-popconfirm>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <!--    分页-->\r\n      <div style=\"margin-top: 20px\">\r\n        <el-pagination\r\n            background\r\n            :current-page=\"params.pageNum\"\r\n            :page-size=\"params.pageSize\"\r\n            layout=\"prev, pager, next\"\r\n            @current-change=\"handleCurrentChange\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n    </el-main>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport Cookies from 'js-cookie'\r\n\r\nexport default {\r\n  name: 'Video',\r\n  data() {\r\n    return {\r\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\r\n      tableData: [],\r\n      total: 0,\r\n      params: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        videoUrl: '',\r\n        stationNumber: '',\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load() {\r\n      this.params.stationNumber = this.admin.stationNumber\r\n      request.get('/video/page', {\r\n        params: this.params\r\n      }).then(res => {\r\n        if (res.code === '200') {\r\n          this.tableData = res.data.list\r\n          this.total = res.data.total\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.params = {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        videoUrl: '',\r\n        stationNumber: this.admin.stationNumber\r\n      }\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      // 点击分页按钮触发分页\r\n      this.params.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    del(videoId) {\r\n      request.delete(\"/video/delete/\" + videoId).then(res => {\r\n        if (res.code === '200') {\r\n          this.$notify.success('删除成功')\r\n          this.load()\r\n        } else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    online(videoId){\r\n      request.put(\"/video/online/\" + videoId).then(res => {\r\n        if (res.code === '200') {\r\n          this.$notify.success('上线成功')\r\n          this.load()\r\n        } else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    handleAdd(){\r\n      this.$router.push(\"/videoAdd\");\r\n    },\r\n    rowStyle() {\r\n      return \"text-align:center\";\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}